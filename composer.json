            "only": [
                "digital-marketing-framework/distributor-request"
            ]
        }
    ],
    "require": {
        "php": "^8.1",
        "digital-marketing-framework/distributor-request": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
